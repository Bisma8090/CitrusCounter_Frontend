{
  "expo": {
    "name": "CitrusCounter",
    "slug": "CitrusCounter",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/citrus_icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
  "image": "./assets/splash_logo.png",
  "resizeMode": "contain",
  "backgroundColor": "#ffffff"
},
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.bismaa.CitrusCounter"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "abdb20ec-1ef3-48b5-8b4e-ac6b1b7d471e"
      }
    }
  }
}
